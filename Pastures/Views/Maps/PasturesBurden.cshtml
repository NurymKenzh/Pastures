    // поиск пастбищ
    function Find() {
        Source_found_burden_pasture.clear();
        var count = 0;
        for (i = 0; i < Layer_burden_pasture_v.getSource().getFeatures().length; i++) {
            var feature = Layer_burden_pasture_v.getSource().getFeatures()[i];
            if ((feature.getProperties().num_vydel != document.getElementById("find_num_vydel").value) && (document.getElementById("find_num_vydel").value != '')) {
                continue;
            }
            //if ((feature.getProperties().rule_grazi != document.getElementById("find_rule_grazi").value) && (document.getElementById("find_rule_grazi").value != '')) {
            //    continue;
            //}
            if ((document.getElementById("find_BurOtdel").options[document.getElementById("find_BurOtdel").selectedIndex].value != '') && (feature.getProperties().bur_otdel != document.getElementById("find_BurOtdel").options[document.getElementById("find_BurOtdel").selectedIndex].value)) {
                continue;
            }
            if ((document.getElementById("find_BType").options[document.getElementById("find_BType").selectedIndex].value != '') && (feature.getProperties().bur_type_i != document.getElementById("find_BType").options[document.getElementById("find_BType").selectedIndex].value)) {
                continue;
            }
            if ((document.getElementById("find_BurSubOtdel").options[document.getElementById("find_BurSubOtdel").selectedIndex].value != '') && (feature.getProperties().bur_subotd != document.getElementById("find_BurSubOtdel").options[document.getElementById("find_BurSubOtdel").selectedIndex].value)) {
                continue;
            }
            if ((document.getElementById("find_BClass").options[document.getElementById("find_BClass").selectedIndex].value != '') && (feature.getProperties().bur_class_ != document.getElementById("find_BClass").options[document.getElementById("find_BClass").selectedIndex].value)) {
                continue;
            }
            if ((document.getElementById("find_BGroup").options[document.getElementById("find_BGroup").selectedIndex].value != '') && (feature.getProperties().bur_group_ != document.getElementById("find_BGroup").options[document.getElementById("find_BGroup").selectedIndex].value)) {
                continue;
            }
            //if ((document.getElementById("find_average_yi_1").value != '') && (parseFloat(feature.getProperties().average_yi) < parseFloat(document.getElementById("find_average_yi_1").value))) {
            //    continue;
            //}
            //if ((document.getElementById("find_average_yi_2").value != '') && (parseFloat(feature.getProperties().average_yi) > parseFloat(document.getElementById("find_average_yi_2").value))) {
            //    continue;
            //}
            if ((document.getElementById("find_burden_gro_1").value != '') && (parseFloat(feature.getProperties().burden_gro) < parseFloat(document.getElementById("find_burden_gro_1").value))) {
                continue;
            }
            if ((document.getElementById("find_burden_gro_2").value != '') && (parseFloat(feature.getProperties().burden_gro) > parseFloat(document.getElementById("find_burden_gro_2").value))) {
                continue;
            }
            if ((document.getElementById("find_burden_deg_1").value != '') && (parseFloat(feature.getProperties().burden_deg) < parseFloat(document.getElementById("find_burden_deg_1").value))) {
                continue;
            }
            if ((document.getElementById("find_burden_deg_2").value != '') && (parseFloat(feature.getProperties().burden_deg) > parseFloat(document.getElementById("find_burden_deg_2").value))) {
                continue;
            }
            Source_found_burden_pasture.addFeature(feature);
            count++;
        }
        //alert(count);
    };

